  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "server": "nodemon ./server/server.js",
    "dev-osx": "export DEV=true && concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "dev-win": "SET DEV=true && concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-mac": "export DEV=true && concurrently \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "package": "npm run build && npm run bundle-server && electron-packager . --overwrite --ignore=\"(.credentials|.gitignore|server/gitignore|src*|server/img/.*|server/.temp|public|server/server.js|server/util.js|/server/local_store.json|/server/downloaded/.*)\"",
    "bundle-server": "browserify --ignore-missing --no-builtins --no-commondir --insert-global-vars=\"global\" --no-browser-field ./server/server.js > ./server/bundle.js",
    "electron-win": "SET DEV=true && electron .",
    "electron-osx": "export DEV=true && electron .",
    "tauri": "tauri"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
